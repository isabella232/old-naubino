    mouseUp="mouseUp(event)"
    mouseMove="mouseMove(event)">
	
	<mx:Canvas x="0" y="0" width="100%" height="100%" id="myCanvas"/>
	
	<mx:Script source="main.as" />
	
</mx:Application>
